{
    "name": "satthi/contents-file",
    "description": "CakePHP ContentsFile",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "file"],
    "homepage": "https://github.com/satthi/Contents-file",
    "license": "MIT",
    "authors": [
        {
            "name": "Satoru Hagiwara",
            "role": "Author"
        }
    ],
    "support": {
        "source": "https://github.com/satthi/Contents-file"
    },
    "require": {
        "cakephp/cakephp": "~5.0",
        "ext-gd": "*",
        "aws/aws-sdk-php": "3.*",
        "symfony/filesystem": "5.*",
        "symfony/finder": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/migrations": "*"
    },
    "autoload": {
        "psr-4": {
            "ContentsFile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContentsFile\\Test\\": "tests",
            "ContentsFile\\Test\\App\\": "tests/test_app/App"
        }
    },
    "suggest": {
    },
    "extra": {
    }
}
